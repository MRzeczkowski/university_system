@model AdminProfileViewModel

<form asp-action="RegisterAdministrator" method="post">
    <input asp-for="UserId" type="hidden"/>
    <div class="form-group">
        <label asp-for="StatusId">Title</label>
        <select asp-for="StatusId" asp-items="Model.StatusOptions" class="form-control"></select>
    </div>
    <br/>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>